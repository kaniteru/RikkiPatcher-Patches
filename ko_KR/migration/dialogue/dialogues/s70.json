        "atts": []
    },
    "109": {
        "speaker": "椎名立希",
        "spans": [
            {
                "html": "style='color:#FfFffF'",
                "text": "一样有贴纸，一样用了很多年。"
            }
        ],
        "atts": {}
    },
    "110": {
        "speaker": "丰川祥子",
        "spans": [
            {
                "html": "style='color:#FfFffF'",
                "text": "......"
            }
        ],
        "atts": {}
    },
    "111": {
        "speaker": "丰川祥子",
        "spans": [
            {
                "html": "style='color:#FfFffF'",
                "text": "那我什么时候还给你？"
