        "atts": {}
    },
    "12": {
        "speaker": "椎名立希",
        "spans": [
            {
                "html": "style='color:#FfFffF'",
                "text": "喂，别开这种玩笑。"
            }
        ],
        "atts": {}
    },
    "13": {
        "speaker": "丰川祥子",
        "spans": [
            {
                "html": "style='color:#FfFffF'",
                "text": "今天我们两个人都很奇怪。"
