{
    "hash": "56216eec9fb1a8a005b43410516d066f3d0cef663c7c47b7c289bf183be7de97213f6034c5c98a4863306b703a97ef6b4582d95385166521dd5554676847e5a3",
    "list": {
        "0": {
            "speaker": "",
            "dialogue": "………"
        },
        "1": {
            "speaker": "",
            "dialogue": "……"
        },
        "2": {
            "speaker": "椎名立希",
            "dialogue": "唔……嗯……"
        },
        "3": {
            "speaker": "椎名立希",
            "dialogue": "唔……"
        },
        "4": {
            "speaker": "",
            "dialogue": "身体的虚弱依旧，动了动身子，黏腻的触感传来，看来是大汗过一场。"
        },
        "5": {
            "speaker": "",
            "dialogue": "但疼痛真的消退了。"
        },
        "6": {
            "speaker": "椎名立希",
            "dialogue": "……"
        },
        "7": {
            "speaker": "椎名立希",
            "dialogue": "（我睡了多久……）"
        },
        "8": {
            "speaker": "",
            "dialogue": "她支起身，所有的关节都传来悲鸣。"
        },
        "9": {
            "speaker": "",
            "dialogue": "餐桌上电子钟已经快要走到8点。"
        },
        "10": {
            "speaker": "",
            "dialogue": "——24小时制的8点。"
        },
        "11": {
            "speaker": "椎名立希",
            "dialogue": "（我睡了……将近一天？！）"
        },
        "12": {
            "speaker": "",
            "dialogue": "立希点亮手机屏幕，日期的数字宣告着事实。"
        },
        "13": {
            "speaker": "椎名立希",
            "dialogue": "（时间……全浪费了……）"
        },
        "14": {
            "speaker": "",
            "dialogue": "天色很暗，外面似乎在下大雨，但不知为何仿佛没有声响。"
        },
        "15": {
            "speaker": "",
            "dialogue": "保持着这个姿势，立希的心情低落到了极点。"
        },
        "16": {
            "speaker": "",
            "dialogue": "不过鼻前的冰凉并没让她失落太久，面前的地板上出现了几个圆点，刺眼的红。"
        },
        "17": {
            "speaker": "椎名立希",
            "dialogue": "！！！"
        },
        "18": {
            "speaker": "",
            "dialogue": "立希慌乱地站起，捂着鼻子冲进浴室。"
        },
        "19": {
            "speaker": "",
            "dialogue": "水龙头被拧至限位，冰冷的水拍在脸上，带走了她的温度。"
        },
        "20": {
            "speaker": "",
            "dialogue": "稀释后的血像丝绸在水中散开，被卷进漆黑的洞口。"
        },
        "21": {
            "speaker": "椎名立希",
            "dialogue": "......哈。"
        },
        "22": {
            "speaker": "",
            "dialogue": "一股深深的无力感揪紧了她。"
        },
        "23": {
            "speaker": "椎名立希",
            "dialogue": "（没时间了……明明已经没时间浪费在睡觉和生病上了……）"
        },
        "24": {
            "speaker": "椎名立希",
            "dialogue": "（……没时间了…为什么？）"
        },
        "25": {
            "speaker": "",
            "dialogue": "不知为何，她觉得自己被一种强烈的急切所驱使，像是正被什么东西追逐，或是在即将坠落的边缘，使人极度坐立不安，心神不宁。"
        },
        "26": {
            "speaker": "椎名立希",
            "dialogue": "（总觉得……刚才梦见了什么……是什么……）"
        },
        "27": {
            "speaker": "",
            "dialogue": "立希努力搜索着昏迷期间的记忆。"
        },
        "28": {
            "speaker": "椎名立希",
            "dialogue": "（还差一点……是什么……和时间有关的……）"
        },
        "29": {
            "speaker": "",
            "dialogue": "时间，已经不多了。"
        },
        "30": {
            "speaker": "",
            "dialogue": "瞬间，她想起来了。"
        },
        "31": {
            "speaker": "",
            "dialogue": "她猛然抬头。"
        },
        "32": {
            "speaker": "",
            "dialogue": "洗手间的镜子因许久未使用，已经蒙上一层灰尘和水垢。立希在模糊间看见镜子上似乎写了一行字。"
        },
        "33": {
            "speaker": "",
            "dialogue": "她用手舀了一捧水，扑在镜面上，胡乱擦了两下，镜面发出刺耳的摩擦声。"
        },
        "34": {
            "speaker": "",
            "dialogue": "然后，她看见了——"
        },
        "35": {
            "speaker": "",
            "dialogue": "立希扑向前，吐了。"
        },
        "36": {
            "speaker": "",
            "dialogue": "报幕"
        }
    }
}