        "atts": []
    },
    "6": {
        "speaker": "Store Clerk",
        "spans": [
            {
                "html": "style='color:#FfFffF'",
                "text": "Hoho~ Stocking up again today? Alright, ten cans of Asahi Dry plus three sets of nutrition jelly packs!"
