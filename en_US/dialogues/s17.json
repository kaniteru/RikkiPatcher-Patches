        "speaker": "",
        "spans": [
            {
                "html": "style='color:#FfFffF'",
                "text": "但她确信，她真的看到这只猫对着她，笑了。"
            }
        ],
        "atts": []
    },
    "79": {
        "speaker": "椎名立希",
        "spans": [
            {
                "html": "style='color:#FfFffF'",
                "text": "…………你…………"
