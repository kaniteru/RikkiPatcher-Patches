        "atts": []
    },
    "21": {
        "speaker": "",
        "spans": [
            {
                "html": "style='color:#FfFffF'",
                "text": "——帽檐之下，有一头亮得恼人的粉色长发。"
            }
        ],
        "atts": []
    },
    "22": {
        "speaker": "椎名立希",
        "spans": [
            {
                "html": "style='color:#FfFffF'",
                "text": "哈？！"
            }
        ],
        "atts": []
    },
    "23": {
        "speaker": "椎名立希",
        "spans": [
            {
                "html": "style='color:#FfFffF'",
                "text": "千早爱音？！？"
