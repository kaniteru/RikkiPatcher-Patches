    "0": {
        "speaker": "",
        "spans": [
            {
                "html": "style='color:#FfFffF'",
                "text": "Just as Taki was thinking about her schedule for the day, her desk suddenly emitted a dull warning."
            }
        ],
        "atts": {
            "dialog": [
                {
                    "font": "en-dia",
                    "fontSize": 34
                },
                {
                    "font": {},
                    "fontSize": {}
